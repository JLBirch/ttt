<region xml:id="横適" tts:writingMode="lrtb" tts:extent="80vw 80vh" tts:position="center" tts:displayAlign="justify"/>
<region xml:id="横中" tts:writingMode="lrtb" tts:extent="80vw 15vh" tts:position="center"/>
<region xml:id="縦右" tts:writingMode="tbrl" tts:extent="15vw 80vh" tts:position="center right 5vh"/>
<region xml:id="縦左" tts:writingMode="tbrl" tts:extent="15vw 80vh" tts:position="center left 5vh"/>
<region xml:id="縦適" tts:writingMode="tbrl" tts:extent="80vw 80vh" tts:position="center" tts:displayAlign="justify"/>
<region xml:id="横中" tts:writingMode="tbrl" tts:extent="15vw 80vh" tts:position="center"/>
</layout>
</head>
</cap2tt>
