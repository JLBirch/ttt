<?xml version="1.0" encoding="utf-8"?>
<config version="1" xmlns="http://skynav.com/ns/ttv">
<option name="add-creation-metadata">false</option>
<option name="default-alignment">centre</option>
<option name="default-kerning">1</option>
<option name="default-language">en</option>
<option name="default-placement">bottom</option>
<option name="default-region">bottom</option>
<option name="default-shear">0</option>
<option name="default-whitespace">preserve</option>
<option name="merge-styles">true</option>
<option name="style-id-pattern">s{0}</option>
<option name="style-id-sequence-start">1</option>
<tt xmlns="http://www.w3.org/ns/ttml" xmlns:tts="http://www.w3.org/ns/ttml#styling">
<head>
<styling>
<initial tts:fontSize="4vh"/>
<initial tts:lineHeight="5vh"/>
<initial tts:showBackground="whenActive"/>
</styling>
<layout>
<region xml:id="bottom" tts:writingMode="lrtb" tts:extent="80vw 15vh" tts:position="center bottom 10vh" tts:displayAlign="after"/>
<region xml:id="top" tts:writingMode="lrtb" tts:extent="80vw 15vh" tts:position="center top 10vh" tts:displayAlign="before"/>
<region xml:id="safe" tts:writingMode="lrtb" tts:extent="80vw 80vh" tts:position="center" tts:displayAlign="justify"/>
<region xml:id="centre" tts:writingMode="lrtb" tts:extent="80vw 15vh" tts:position="center" tts:displayAlign="center"/>
</layout>
</head>
</tt>
</config>
